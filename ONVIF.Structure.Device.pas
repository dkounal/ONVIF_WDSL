unit ONVIF.Structure.Device;

interface
  Type
  /// <summary>
  ///   Represents information about a device, including manufacturer, model, firmware version,
  ///   serial number, hardware ID, and XAddr.
  /// </summary>
  TDeviceInformation = record
    Manufacturer   : string;
    Model          : string;
    FirmwareVersion: String;
    SerialNumber   : String;
    HardwareId     : String;
    XAddr          : String;
  end;  

